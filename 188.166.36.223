--

CREATE FUNCTION public.fnc_auditlog_users_insert() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
INSERT INTO auditlog(user_id,creation_time)
SELECT user_id,record_date
FROM users GROUP BY user_id
HAVING user_id = currval(pg_get_serial_sequence('users','user_id'));
RETURN NULL;
END;
$$;


